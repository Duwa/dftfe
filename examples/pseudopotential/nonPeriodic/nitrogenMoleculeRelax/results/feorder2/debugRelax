Usage:
./dftRun parameterfile.prm (OR) mpirun -np nProcs ./dftRun parameterfile.prm

# Listing of Parameters
# ---------------------
# Number of SCF iterations to be considered for mixing the electron-density
set ANDERSON SCHEME MIXING HISTORY                = 70

# Mixing parameter to be used in Anderson scheme
set ANDERSON SCHEME MIXING PARAMETER              = 0.5

# File specifying the coordinates of the atoms in the given material system
set ATOMIC COORDINATES FILE                       = coordinates.inp    # default: 

# Boolean parameter specifying if cell stress is to be relaxed
set CELL OPT                                      = false

# The degree of the Chebyshev polynomial to be employed for filtering out the
# unwanted spectrum (Default value is used when the input parameter value is
# 0
set CHEBYSHEV POLYNOMIAL DEGREE                   = 0

# Path specifying the location of the build directory
set DFT PATH                                      = "../../../../"     # default: 

# Size of the domain along 1-direction
set DOMAIN SIZE X                                 = 80.0               # default: 0.0

# Size of the domain along 2-direction
set DOMAIN SIZE Y                                 = 80.0               # default: 0.0

# Size of the domain along 3-direction
set DOMAIN SIZE Z                                 = 80.0               # default: 0.0

# Parameter specifying the type of exchange-correlation to be used
set EXCHANGE CORRELATION TYPE                     = 1

# The degree of the finite-element interpolating polynomial
set FINITE ELEMENT POLYNOMIAL ORDER               = 2

# Inner Domain Size along 1-direction
set INNER DOMAIN SIZE X                           = 0.0

# Inner Domain Size along 2-direction
set INNER DOMAIN SIZE Y                           = 0.0

# Inner Domain Size along 3-direction
set INNER DOMAIN SIZE Z                           = 0.0

# Boolean parameter specifying if atomic forces are to be relaxed
set ION OPT                                       = true               # default: false

# File specifying the lattice vectors associated with the unit-cell
set LATTICE VECTORS FILE                          = latticeVectors.inp # default: 

# The lower bound of the wanted eigen spectrum
set LOWER BOUND WANTED SPECTRUM                   = -10.0

# Finite-element mesh file to be used for the given problem
set MESH FILE                                     = 

# Inner Domain Mesh Size
set MESH SIZE INNER DOMAIN                        = 0.625              # default: 0.0

# Mesh Size near atom
set MESH SIZE NEAR ATOM                           = 0.2                # default: 0.0

# Mesh Size in a ball around atom
set MESH SIZE OUTER ATOM BALL                     = 2.0                # default: 0.0

# Outer Domain Mesh Size
set MESH SIZE OUTER DOMAIN                        = 10.0               # default: 0.0

# Number of Kohn-Sham wavefunctions to be computed
set NUMBER OF KOHN-SHAM WAVEFUNCTIONS             = 12                 # default: 10

# Number of refinement steps to be used
set NUMBER OF REFINEMENT STEPS                    = 4

# Flag to control optimized/debug modes
set OPTIMIZED MODE                                = true

# Radius of outer ball enclosing atom
set OUTER ATOM BALL RADIUS                        = 3                  # default: 0.0

# Periodicity in X-direction
set PERIODIC BOUNDARY CONDITION X                 = false

# Periodicity in Y-direction
set PERIODIC BOUNDARY CONDITION Y                 = false

# Periodicity in Z-direction
set PERIODIC BOUNDARY CONDITION Z                 = false

# Maximum number of iterations to be allowed for Poisson problem convergence
set POISSON SOLVER CONVERGENCE MAXIMUM ITERATIONS = 10000              # default: 5000

# Relative tolerance as stopping criterion for Poisson problem convergence
set POISSON SOLVER CONVERGENCE TOLERANCE          = 1e-12

# Boolean Parameter specifying whether pseudopotential DFT calculation needs
# to be performed
set PSEUDOPOTENTIAL CALCULATION                   = true               # default: false

# Maximum number of iterations to be allowed for SCF convergence
set SCF CONVERGENCE MAXIMUM ITERATIONS            = 100                # default: 50

# SCF iterations stopping tolerance in terms of electron-density difference
# between two successive iterations
set SCF CONVERGENCE TOLERANCE                     = 1e-9               # default: 1e-08

# The radius of the ball around an atom on which self-potential of the
# associated nuclear charge is solved
set SELF POTENTIAL ATOM BALL RADIUS               = 8.0                # default: 3.0

# Fermi-Dirac smearing temperature
set TEMPERATURE                                   = 500                # default: 500.0

# File specifying the k-Point quadrature rule to sample Brillouin zone
set kPOINT RULE FILE                              = GammaPoint.inp     # default: 

