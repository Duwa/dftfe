Usage:
./dftRun parameterfile.prm (OR) mpirun -np nProcs ./dftRun parameterfile.prm

# Listing of Parameters
# ---------------------
# Number of SCF iterations to be considered for mixing the electron-density
set ANDERSON SCHEME MIXING HISTORY                = 70

# Mixing parameter to be used in Anderson scheme
set ANDERSON SCHEME MIXING PARAMETER              = 0.5

# File specifying the coordinates of the atoms in the given material system
set ATOMIC COORDINATES FILE                       = coordinates.inp               # default: 

# Number of Monkhorts-Pack grid points to be used along X direction for BZ
# sampling
set BZ SAMPLING POINTS ALONG X                    = 2

# Number of Monkhorts-Pack grid points to be used along Y direction for BZ
# sampling
set BZ SAMPLING POINTS ALONG Y                    = 2

# Number of Monkhorts-Pack grid points to be used along Z direction for BZ
# sampling
set BZ SAMPLING POINTS ALONG Z                    = 2

# Fractional shifting to be used along X direction for BZ sampling
set BZ SAMPLING SHIFT ALONG X                     = 0.25                          # default: 0.0

# Fractional shifting to be used along Y direction for BZ sampling
set BZ SAMPLING SHIFT ALONG Y                     = 0.25                          # default: 0.0

# Fractional shifting to be used along Z direction for BZ sampling
set BZ SAMPLING SHIFT ALONG Z                     = 0.25                          # default: 0.0

# Type of the constraint to be used during cell stress relaxation
set CELL CONSTRAINT TYPE                          = 1                             # default: 12

# Boolean parameter specifying if cell stress is to be relaxed
set CELL OPT                                      = true                          # default: false

# Boolean parameter specifying if cell stress is to be computed
set CELL STRESS                                   = true                          # default: false

# The number of the Chebyshev filter passes per SCF  (Default value is used
# when the input parameter is not specified
set CHEBYSHEV FILTER PASSES                       = 1

# The degree of the Chebyshev polynomial to be employed for filtering out the
# unwanted spectrum (Default value is used when the input parameter value is
# 0
set CHEBYSHEV POLYNOMIAL DEGREE                   = 0

# Path specifying the location of the build directory
set DFT PATH                                      = "/home/dsambit/dftfe/pullreq" # default: 

# File specifying the domain bounding vectors associated with the domain. In
# fully periodic case, these are the lattice vectors.
set DOMAIN BOUNDING VECTORS FILE                  = latticeVectors.inp            # default: 

# Parameter specifying the type of exchange-correlation to be used
set EXCHANGE CORRELATION TYPE                     = 1

# The degree of the finite-element interpolating polynomial
set FINITE ELEMENT POLYNOMIAL ORDER               = 4                             # default: 2

# Sets the tolerance of the maximum force (in atomic units) on an ion when
# forces are considered to be relaxed.
set FORCE TOL                                     = 5e-5

# Inner Domain Size along 1-direction
set INNER DOMAIN SIZE X                           = 0.0

# Inner Domain Size along 2-direction
set INNER DOMAIN SIZE Y                           = 0.0

# Inner Domain Size along 3-direction
set INNER DOMAIN SIZE Z                           = 0.0

# Boolean parameter specifying if atomic forces are to be computed
set ION FORCE                                     = true                          # default: false

# Boolean parameter specifying if atomic forces are to be relaxed
set ION OPT                                       = false

# File specifying the atomic position update permission flags. 1- update 0- no
# update
set ION RELAX FLAGS FILE                          = relaxationFlags.inp           # default: 

# The lower bound of the wanted eigen spectrum
set LOWER BOUND WANTED SPECTRUM                   = -70.0                         # default: -10.0

# Finite-element mesh file to be used for the given problem
set MESH FILE                                     = 

# Inner Domain Mesh Size
set MESH SIZE INNER DOMAIN                        = 1.0                           # default: 0.0

# Mesh Size near atom
set MESH SIZE NEAR ATOM                           = 0.5                           # default: 0.0

# Mesh Size in a ball around atom
set MESH SIZE OUTER ATOM BALL                     = 0.5                           # default: 0.0

# Outer Domain Mesh Size
set MESH SIZE OUTER DOMAIN                        = 2.0                           # default: 0.0

# Number of Kohn-Sham wavefunctions to be computed
set NUMBER OF KOHN-SHAM WAVEFUNCTIONS             = 40                            # default: 10

# Number of pools to be generated for k-point parallelization
set NUMBER OF POOLS                               = 2                             # default: 1

# Number of refinement steps to be used
set NUMBER OF REFINEMENT STEPS                    = 4

# Flag to control optimized/debug modes
set OPTIMIZED MODE                                = true

# Radius of outer ball enclosing atom
set OUTER ATOM BALL RADIUS                        = 2.0                           # default: 0.0

# Periodicity in X-direction
set PERIODIC BOUNDARY CONDITION X                 = true                          # default: false

# Periodicity in Y-direction
set PERIODIC BOUNDARY CONDITION Y                 = true                          # default: false

# Periodicity in Z-direction
set PERIODIC BOUNDARY CONDITION Z                 = true                          # default: false

# Maximum number of iterations to be allowed for Poisson problem convergence
set POISSON SOLVER CONVERGENCE MAXIMUM ITERATIONS = 10000                         # default: 5000

# Relative tolerance as stopping criterion for Poisson problem convergence
set POISSON SOLVER CONVERGENCE TOLERANCE          = 1e-12

# Boolean Parameter specifying whether pseudopotential DFT calculation needs
# to be performed
set PSEUDOPOTENTIAL CALCULATION                   = true                          # default: false

# Type of nonlocal projector to be used: 1 for KB, 2 for ONCV, default is KB
set PSEUDOPOTENTIAL TYPE                          = 1

# Flag to control whether to read symmetries supplied by user
set READ SYMMETRY FROM FILE                       = false

# Maximum number of iterations to be allowed for SCF convergence
set SCF CONVERGENCE MAXIMUM ITERATIONS            = 100                           # default: 50

# SCF iterations stopping tolerance in terms of electron-density difference
# between two successive iterations
set SCF CONVERGENCE TOLERANCE                     = 1e-7                          # default: 1e-08

# The radius of the ball around an atom on which self-potential of the
# associated nuclear charge is solved
set SELF POTENTIAL ATOM BALL RADIUS               = 2.5                           # default: 3.0

# Is spin polarization to be included?
set SPIN POLARIZATION                             = 0

# Magnetization to start with
set START MAGNETIZATION                           = 0.0

# Sets the tolerance of the cell stress (in atomic units) when cell stress is
# considered to be relaxed.
set STRESS TOL                                    = 5e-7

# File specifying the symmetry matrices for obtaining the irreducible BZ
set SYMMETRY MATRIX FILE                          = 

# Fermi-Dirac smearing temperature
set TEMPERATURE                                   = 500                           # default: 500.0

# Flag to control usage of space group symmetries (only for periodic
# calculation)
set USE GROUP SYMMETRY                            = false                         # default: true

# Flag to control usage of time reversal symmetry
set USE TIME REVERSAL SYMMETRY                    = true                          # default: false

# File specifying the k-Point quadrature rule to sample Brillouin zone
set kPOINT RULE FILE                              = 
