# Listing of Parameters
# ---------------------
# File specifying the coordinates of the atoms in the given material system
set ATOMIC COORDINATES FILE                       = coordinates.inp                                    # default: 

# The degree of the Chebyshev polynomial to be employed for filtering out the
# unwanted spectrum
set CHEBYSHEV POLYNOMIAL DEGREE                   = 50                                                 # default: 0

# Path specifying the location of the build directory
set DFT PATH                                      = "/home/phanim/KohnShamCodes/DEALIICode/dftfeBuild" # default: 

# Size of the domain in X-direction
set DOMAIN SIZE X                                 = 7.6                                                # default: 0.0

# Size of the domain in Y-direction
set DOMAIN SIZE Y                                 = 7.6                                                # default: 0.0

# Size of the domain in Z-direction
set DOMAIN SIZE Z                                 = 7.6                                                # default: 0.0

# Parameter specifying the type of exchange-correlation to be used
set EXCHANGE CORRELATION TYPE                     = 1

# The degree of the finite-element interpolating polynomial
set FINITE ELEMENT POLYNOMIAL ORDER               = 4                                                  # default: 2

# File specifying the lattice vectors associated with the unit-cell
set LATTICE VECTORS FILE                          = latticeVectors.inp                                 # default: 

# The lower bound of the wanted eigen spectrum
set LOWER BOUND WANTED SPECTRUM                   = -10.0

# Finite-element mesh file to be used for the given problem
set MESH FILE                                     = mesh.inp                                           # default: 

# Number of Kohn-Sham wavefunctions to be computed
set NUMBER OF KOHN-SHAM WAVEFUNCTIONS             = 10

# Number of refinement steps to be used
set NUMBER OF REFINEMENT STEPS                    = 4

# Flag to control optimized/debug modes
set OPTIMIZED MODE                                = true

# Periodicity in X-direction
set PERIODIC BOUNDARY CONDITION X                 = true                                               # default: false

# Periodicity in Y-direction
set PERIODIC BOUNDARY CONDITION Y                 = true                                               # default: false

# Periodicity in Z-direction
set PERIODIC BOUNDARY CONDITION Z                 = true                                               # default: false

# Maximum number of iterations to be allowed for Poisson problem convergence
set POISSON SOLVER CONVERGENCE MAXIMUM ITERATIONS = 5000

# Relative tolerance as stopping criterion for Poisson problem convergence
set POISSON SOLVER CONVERGENCE TOLERANCE          = 1e-12

# Boolean Parameter specifying whether pseudopotential DFT calculation needs
# to be performed
set PSEUDOPOTENTIAL CALCULATION                   = true                                               # default: false

# The radius of the ball around an atom on which self-potential of the
# associated nuclear charge is solved
set RADIUS ATOM BALL                              = 3.0

# Maximum number of iterations to be allowed for SCF convergence
set SCF CONVERGENCE MAXIMUM ITERATIONS            = 30                                                 # default: 50

# SCF iterations stopping tolerance in terms of electron-density difference
# between two successive iterations
set SCF CONVERGENCE TOLERANCE                     = 1e-11                                              # default: 1e-08

# Fermi-Dirac smearing temperature
set TEMPERATURE                                   = 500                                                # default: 500.0

